<?xml version="1.0" encoding="utf-8"?>
<Window >
  <Style name="blockcalcPageOptionStyle" value="width=&quot;60&quot; group=&quot;blockPageBlockGroup&quot; textcolor=&quot;#FF222222&quot; selectedtextcolor=&quot;#FFff0000&quot; pushedtextcolor=&quot;#FFFF0000&quot; "/>
  
  <BlockcalcPage bkcolor="#FFFFFFFF">
	<HorizontalLayout height="30" inset="1,1,1,1" bordersize="0,0,0,1" bordercolor="#ff999999">
		<HorizontalLayout width="250" childvalign="vcenter" bordersize="0,0,1,0" bordercolor="#ff999999">
			<Option name="blockcalcPageOptBuy" text="买入条件" style="blockcalcPageOptionStyle" />
			<Option name="blockcalcPageOptSell" text="不买条件" style="blockcalcPageOptionStyle" />
			<Option name="blockcalcPageOptSelf" text="自选板块" style="blockcalcPageOptionStyle" />
			<Option name="blockcalcPageOptSelect" text="选股条件" style="blockcalcPageOptionStyle" />
		</HorizontalLayout>
		<HorizontalLayout childvalign="vcenter" bordersize="1,0,0,0" bordercolor="#ff999999">
			<Control />
			<Button name="blockcalcPageBtnLoad"  text="加载" width="50" height="26" padding="10,0,0,0" />
			<Button name="blockcalcPageBtnUpdateAll"  text="下载" width="50" height="26" padding="10,0,10,0" />
		</HorizontalLayout>
		<HorizontalLayout width="250" childvalign="vcenter" bordersize="1,0,0,0" bordercolor="#ff999999">
			<Button name="blockcalcPageBtnModify"  text="修改" width="50" height="26" padding="10,0,0,0" />
			<Button name="blockcalcPageBtnNew"  text="新增" width="50" height="26" padding="10,0,0,0" />
			<Button name="blockcalcPageBtnDelete"  text="删除" width="50" height="26" padding="10,0,0,0" />
			<Button name="blockcalcPageBtnSave"  text="保存" width="50" height="26" padding="10,0,10,0" />
		</HorizontalLayout>
		<HorizontalLayout width="250" childvalign="vcenter" bordersize="1,0,0,0" bordercolor="#ff999999">
			<Button name="blockcalcPageBtnUpdate"  text="更新" width="50" height="26" padding="10,0,0,0" />
			<Button name="blockcalcPageBtnDelStock"  text="移除" width="50" height="26" padding="10,0,0,0" />
			<Button name="blockcalcPageBtnAddStock"  text="添加" width="50" height="26" padding="10,0,0,0" />
			<Button name="blockcalcPageBtnExportStock"  text="导出" width="50" height="26" padding="10,0,0,0" />
		</HorizontalLayout>
	</HorizontalLayout>
	<HorizontalLayout>
		<TabLayout name="blockcalcPageBlockSwitch" selectedid="0" width="200" bordersize="0,0,1,0" bordercolor="#ff999999">
			<List name="blockcalcPageListBuy" header="hidden"  inset="1,1,1,1" vscrollbar="true" itemtextcolor="#ff506a87" itemhottextcolor="#ff506a87" itemselectedtextcolor="#ff506a87" itemtextpadding="4" itemendellipsis="true">
			</List>
			<List name="blockcalcPageListSell" header="hidden"  inset="1,1,1,1" vscrollbar="true" itemtextcolor="#ff506a87" itemhottextcolor="#ff506a87" itemselectedtextcolor="#ff506a87" itemtextpadding="4" itemendellipsis="true">
			</List>
			<List name="blockcalcPageListSelf" header="hidden"  inset="1,1,1,1" vscrollbar="true" itemtextcolor="#ff506a87" itemhottextcolor="#ff506a87" itemselectedtextcolor="#ff506a87" itemtextpadding="4" itemendellipsis="true">
			</List>
			<List name="blockcalcPageListSelect" header="hidden"  inset="1,1,1,1" vscrollbar="true" itemtextcolor="#ff506a87" itemhottextcolor="#ff506a87" itemselectedtextcolor="#ff506a87" itemtextpadding="4" itemendellipsis="true">
			</List>
		</TabLayout>
		<List name="blockcalcPageListStock" width="120" bordersize="0,0,1,0" bordercolor="#ff999999" header="hidden"  inset="1,1,1,1" vscrollbar="true" itemtextcolor="#ff506a87" itemhottextcolor="#ff506a87" itemselectedtextcolor="#ff506a87" itemtextpadding="4" itemendellipsis="true">
		</List>
		<VerticalLayout bkcolor="#ffffffff">
			<HorizontalLayout height="30" childvalign="vcenter" bordersize="0,0,0,1" bordercolor="#ff999999">
				<Button name="blockcalcPageBtnUnion"  text="并集" width="50" height="26" padding="10,0,0,0" />
				<Button name="blockcalcPageBtnInner"  text="交集" width="50" height="26" padding="10,0,0,0" />
				<Button name="blockcalcPageBtnOut"  text="差集" width="50" height="26" padding="10,0,0,0" />
				<Button name="blockcalcPageBtnLoadCalc"  text="加载" width="50" height="26" padding="10,0,0,0" />
				<Button name="blockcalcPageBtnDeleteCalc"  text="删除" width="50" height="26" padding="10,0,0,0" />
				<Button name="blockcalcPageBtnCalcAddTo"  text="添加到" width="50" height="26" padding="10,0,0,0" />
			</HorizontalLayout>
			<HorizontalLayout>
				<List name="blockcalcPageListCalcBlock"  width="200" header="hidden" inset="1,1,1,1" vscrollbar="true" itemtextcolor="#ff506a87" itemhottextcolor="#ff506a87" itemselectedtextcolor="#ff506a87" itemtextpadding="4" itemendellipsis="true" bordersize="0,0,1,0" bordercolor="#ff999999">
				</List>
				<List name="blockcalcPageListCalcStock" width="120" bordersize="0,0,1,0" bordercolor="#ff999999" header="hidden"  inset="1,1,1,1" vscrollbar="true" itemtextcolor="#ff506a87" itemhottextcolor="#ff506a87" itemselectedtextcolor="#ff506a87" itemtextpadding="4" itemendellipsis="true">
				</List>
			</HorizontalLayout>
		</VerticalLayout>
	</HorizontalLayout>
  </BlockcalcPage>
</Window>